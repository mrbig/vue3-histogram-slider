{
  "name": "vue3-histogram-slider",
  "version": "0.4.15",
  "description": "Range slider with histogram for Vue 3",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --name histogram-slider --entry ./src/lib/HistogramSlider.vue --target lib",
    "lint": "vue-cli-service lint",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "main": "dist/histogram-slider.umd.min.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "d3-array": "^2.3.2",
    "d3-brush": "^1.1.3",
    "d3-scale": "^3.1.0",
    "d3-selection": "^1.4.0",
    "d3-transition": "^1.2.0",
    "jquery": "3.5",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^8.1.5",
    "prettier": "^2.2.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "vue",
    "vuejs",
    "slider",
    "range-slider",
    "histogram",
    "histogram-slider"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phedone/vue3-histogram-slider.git"
  }
}
